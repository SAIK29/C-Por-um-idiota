#include <iostream>
#include <string>

int main() {
 
	//int, é utilizado para guardar valores de números inteiros.
	int X = 29;
	//double, é utilizado para guardar valores de números com virgulas, diferente de python ou C#.
	double Y = 30.29;
	//char, é utilizado para guardar 1 caracteres.
	char N = 'A';
	//bool, só tem 2 estados verdadeiro ou falso.
	bool R = true;
	//string, é usado para armazenar uma sequencia de caracteres.
	std::string W = "iorgute";
	//Note.EX: os acentos e certos caracteres aparecem de forma errada.
	//const, é usado para travar o valor de uma variável.
	const bool L = false;

	std::cout << "Eu gosto de " << W << std::endl;
	
	system("pause");

	return 0;
}
